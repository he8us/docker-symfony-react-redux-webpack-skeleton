{
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "promise"
    ],
    "ecmaFeatures": {
        "classes": true,
        "jsx": true
    },
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "no-console": 0
    }
}
